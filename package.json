{
  "name": "nitro-project",
  "version": "0.0.0",
  "description": "Nitro is a Node.js application for simple and complex frontend development with a tiny footprint. This app is based on generator-nitro 3.5.0",
  "license": "UNLICENSED",
  "private": true,
  "author": "The Nitro Team",
  "engines": {
    "node": ">=8 <11",
    "npm": ">=5 <6"
  },
  "scripts": {
    "create-pattern": "yo nitro:pattern",
    "nitro:pattern": "yarn create-pattern",
    "nitro:update": "yarn add generator-nitro --exact && yo nitro",
    "githooks": "gulp sync-githooks",
    "lint": "npm-run-all lint:*",
    "lint:accessibility": "gulp lint-accessibility",
    "lint:css": "stylelint src/patterns/**/*.*ss",
    "lint:data": "node app/scripts/validate-pattern-data.js",
    "lint:html": "gulp lint-html",
    "lint:js": "eslint ./src/patterns ./src/share ./src/*.js",
    "lint:license": "license-checker --production --summary --exclude \"Apache-2.0, BSD, ISC, LGPL, MIT, MPL\" --failOn \"AGPL; EPL; GPL\"",
    "postinstall": "npm rebuild node-sass && yarn githooks",
    "prepublish": "nsp check --output summary",
    "visual-approve": "gulp visual-approve",
    "visual-reference": "gulp visual-reference",
    "visual-test": "gulp visual-test",
    "test": "npm-run-all test:*",
    "test:lint": "yarn lint",
    "test:karma": "gulp test",
    "XXXprebuild": "yarn test",

    "build": "npm-run-all build:*",
    "build:clean": "rimraf public/assets",
    "build:assets": "gulp assets",
    "build:webpack": "webpack --mode production --config config/webpack/webpack.config.prod.js",

    "export": "yarn run build && gulp export",

    "dev": "cross-env WEBPACK_CONFIG=../../config/webpack/webpack.config.dev.js gulp develop",

    "prod": "npm-run-all prod:*",
    "prod:build": "yarn build",
    "prod:serve": "cross-env NODE_ENV=production gulp serve",

    "start": "npm-run-all start:*",
    "start:dev": "yarn dev",

    "XXXbuild-dev": "webpack --mode development --config config/webpack/webpack.config.dev.js",
    "XXXstart-dev": "cross-env WEBPACK_CONFIG=../../config/webpack/webpack.config.dev.js node server"
  },
  "keywords": [
    "frontend",
    "app",
    "nitro",
    "component",
    "pattern",
    "modular"
  ],
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "handlebars": "4.0.11",
    "jquery": "3.3.1",
    "lodash": "4.17.5",
    "terrific": "3.0.0"
  },
  "devDependencies": {
    "@namics/eslint-config": "5.0.0",
    "@namics/stylelint-bem": "5.0.0",
    "ajv": "6.4.0",
    "autoprefixer": "8.3.0",
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "backstopjs": "3.2.15",
    "body-parser": "1.18.2",
    "browser-sync": "2.23.7",
    "chalk": "2.4.0",
    "compression": "1.7.2",
    "config": "1.30.0",
    "cross-env": "5.1.4",
    "del": "3.0.0",
    "dot-object": "1.7.0",
    "es6-promise": "4.2.4",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.11.0",
    "express": "4.16.3",
    "extend": "3.0.1",
    "generator-nitro": "3.5.1",
    "get-port": "3.2.0",
    "globby": "7.1.1",
    "gulp": "3.9.1",
    "gulp-accessibility": "3.1.1",
    "gulp-cached": "1.1.1",
    "gulp-change": "1.0.0",
    "gulp-declare": "0.3.0",
    "gulp-header": "2.0.5",
    "gulp-htmllint": "0.0.15",
    "gulp-imagemin": "4.1.0",
    "gulp-live-server": "0.0.31",
    "gulp-load-plugins": "1.5.0",
    "gulp-newer": "1.4.0",
    "gulp-plumber": "1.2.0",
    "gulp-remote-src": "0.4.3",
    "gulp-rename": "1.2.2",
    "gulp-sequence": "1.0.0",
    "gulp-svgmin": "1.2.4",
    "gulp-svgstore": "6.1.1",
    "gulp-watch": "5.0.0",
    "gulp-wrap": "0.13.0",
    "hbs": "4.0.1",
    "hbs-utils": "0.0.4",
    "htmllint": "0.7.2",
    "i18next": "11.2.3",
    "i18next-express-middleware": "1.1.1",
    "i18next-node-fs-backend": "1.0.0",
    "i18next-sprintf-postprocessor": "0.2.2",
    "imagemin-pngquant": "5.1.0",
    "jasmine-core": "3.1.0",
    "karma": "2.0.2",
    "karma-babel-preprocessor": "7.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.1",
    "karma-phantomjs-launcher": "1.0.4",
    "license-checker": "18.0.0",
    "merge-stream": "1.0.1",
    "nitro-exporter": "1.0.0",
    "npm-run-all": "4.1.2",
    "nsp": "3.2.1",
    "stylelint": "9.2.0",
    "text-table": "0.2.0",
    "yargs": "11.0.0",
    "yo": "2.0.2",


    "rimraf": "2.6.2",

    "webpack": "4.6.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-middleware": "3.1.2",
    "webpack-hot-middleware": "2.22.1",

    "mini-css-extract-plugin": "0.4.0",
    "sass-loader": "7.0.1",
    "node-sass": "4.8.3",
    "style-loader": "0.21.0",
    "css-loader": "0.28.11",
    "postcss-loader": "2.1.4",
    "stylelint-webpack-plugin": "0.10.4",

    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.0.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "optimize-css-assets-webpack-plugin": "4.0.0",

    "babel-loader": "7.1.4",
    "handlebars-loader": "1.7.0",
    "babel-preset-react": "6.24.1",
    "react": "16.3.2",
    "react-dom": "16.3.2"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "ie 11",
    "android 4",
    "Firefox ESR"
  ]
}
