{
  "name": "nitro-project",
  "version": "0.0.0",
  "description": "Nitro is a Node.js application for simple and complex frontend development with a tiny footprint. This app is based on generator-nitro 3.5.0",
  "license": "UNLICENSED",
  "private": true,
  "author": "The Nitro Team",
  "engines": {
    "node": ">=6 <9",
    "npm": ">=3 <6"
  },
  "scripts": {
    "create-pattern": "yo nitro:pattern",
    "githooks": "gulp sync-githooks",
    "lint": "npm-run-all lint:*",
    "lint:accessibility": "gulp lint-accessibility",
    "lint:css": "stylelint src/assets/**/*.*ss src/patterns/**/*.*ss",
    "lint:data": "node app/scripts/validate-pattern-data.js",
    "lint:html": "gulp lint-html",
    "lint:js": "eslint ./src/patterns ./src/assets/js",
    "lint:license": "license-checker --production --summary --exclude \"Apache-2.0, BSD, ISC, LGPL, MIT, MPL\" --failOn \"AGPL; EPL; GPL\"",
    "postinstall": "npm rebuild node-sass && yarn githooks",
    "prepublish": "nsp check --output summary",
    "visual-approve": "gulp visual-approve",
    "visual-reference": "gulp visual-reference",
    "visual-test": "gulp visual-test",
    "test": "npm-run-all test:*",
    "test:lint": "yarn lint",
    "test:karma": "gulp test",
    "prebuild": "yarn test",
    "build": "gulp build",
    "export": "gulp export",
    "dev": "gulp develop",
    "prod": "cross-env NODE_ENV=production gulp production",
    "start": "yarn dev"
  },
  "keywords": [
    "frontend",
    "app",
    "nitro",
    "component",
    "pattern",
    "modular"
  ],
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "handlebars": "4.0.11",
    "jquery": "3.3.1",
    "terrific": "3.0.0"
  },
  "devDependencies": {
    "@namics/eslint-config": "5.0.0",
    "@namics/stylelint-bem": "5.0.0",
    "ajv": "6.4.0",
    "autoprefixer": "8.3.0",
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "backstopjs": "3.1.21",
    "body-parser": "1.18.2",
    "browser-sync": "2.23.6",
    "chalk": "2.4.0",
    "compression": "1.7.2",
    "config": "1.30.0",
    "cross-env": "5.1.4",
    "del": "3.0.0",
    "dot-object": "1.7.0",
    "es6-promise": "4.2.4",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.11.0",
    "express": "4.16.3",
    "extend": "3.0.1",
    "generator-nitro": "3.5.0",
    "get-port": "3.2.0",
    "globby": "7.1.1",
    "gulp": "3.9.1",
    "gulp-accessibility": "3.1.1",
    "gulp-babel": "7.0.1",
    "gulp-cached": "1.1.1",
    "gulp-change": "1.0.0",
    "gulp-concat": "2.6.1",
    "gulp-cssnano": "2.1.3",
    "gulp-declare": "0.3.0",
    "gulp-eslint": "4.0.2",
    "gulp-handlebars": "5.0.2",
    "gulp-header": "2.0.5",
    "gulp-htmllint": "0.0.15",
    "gulp-if": "2.0.2",
    "gulp-imagemin": "4.1.0",
    "gulp-live-server": "0.0.31",
    "gulp-load-plugins": "1.5.0",
    "gulp-newer": "1.4.0",
    "gulp-plumber": "1.2.0",
    "gulp-postcss": "7.0.1",
    "gulp-remember": "1.0.1",
    "gulp-remote-src": "0.4.3",
    "gulp-rename": "1.2.2",
    "gulp-sass": "4.0.1",
    "gulp-sequence": "1.0.0",
    "gulp-size": "3.0.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-stylelint": "3.9.0",
    "gulp-svgmin": "1.2.4",
    "gulp-svgstore": "6.1.1",
    "gulp-uglify": "3.0.0",
    "gulp-watch": "5.0.0",
    "gulp-wrap": "0.13.0",
    "hbs": "4.0.1",
    "hbs-utils": "0.0.4",
    "htmllint": "0.7.2",
    "i18next": "11.2.3",
    "i18next-express-middleware": "1.1.1",
    "i18next-node-fs-backend": "1.0.0",
    "i18next-sprintf-postprocessor": "0.2.2",
    "imagemin-pngquant": "5.1.0",
    "jasmine-core": "3.1.0",
    "karma": "2.0.0",
    "karma-babel-preprocessor": "7.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.1",
    "karma-phantomjs-launcher": "1.0.4",
    "license-checker": "18.0.0",
    "merge-stream": "1.0.1",
    "nitro-exporter": "1.0.0",
    "npm-run-all": "4.1.2",
    "nsp": "3.2.1",
    "stylelint": "9.2.0",
    "text-table": "0.2.0",
    "yargs": "11.0.0",
    "yo": "2.0.2"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "ie 11",
    "android 4",
    "Firefox ESR"
  ]
}
